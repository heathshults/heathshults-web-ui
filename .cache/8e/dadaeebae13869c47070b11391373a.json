ted":{"line":58,"column":0},"source":"modules/theme-switcher/theme-switcher.js","original":{"line":55,"column":0}},{"name":"_ThemeSwitcher","generated":{"line":58,"column":4},"source":"modules/theme-switcher/theme-switcher.js","original":{"line":55,"column":6}},{"generated":{"line":58,"column":18},"source":"modules/theme-switcher/theme-switcher.js","original":{"line":55,"column":20}},{"name":"ThemeSwitcher","generated":{"line":58,"column":21},"source":"modules/theme-switcher/theme-switcher.js","original":{"line":55,"column":21}},{"generated":{"line":58,"column":34},"source":"modules/theme-switcher/theme-switcher.js","original":{"line":55,"column":0}},{"name":"ThemeSwitcher","generated":{"line":60,"column":0},"source":"modules/theme-switcher/theme-switcher.js","original":{"line":57,"column":0}},{"generated":{"line":60,"column":13},"source":"modules/theme-switcher/theme-switcher.js","original":{"line":57,"column":13}}],"sources":{"modules/theme-switcher/theme-switcher.js":"/* eslint-disable no-console */\nfunction ThemeSwitcher() {\n  \n  // ** ====== MODE WIDHET ====== ** //\n  // (() => {\n    var $dm_btn      = $('#mode_widget');\n    var lsGetMode   = localStorage.getItem('dark_mode');\n\n    // set button text\n      if (lsGetMode === 'fasle') {\n        setModeText(true);\n        //$dm_btn.html('<span class=\"which-mode\">Dark Mode<span id=\"mode_icon\" class=\"fa fa-moon-o mode-icon\"></span></span>')\n      } else {\n        setModeText(false);\n        // $dm_btn.html('<span class=\"which-mode\">Light Mode<span id=\"mode_icon\" class=\"fa fa-sun-o mode-icon\"></span></span>')\n      }\n\n    function setModeText(mode) {\n      let darkHTML = '<span class=\"which-mode\"> Dark Mode<i id=\"mode_icon\" class=\"fa fa-moon mode-icon\"></i></span>';\n      let lightHTML = '<span class=\"which-mode\"> Light Mode<i id=\"mode_icon\" class=\"fa fa-sun mode-icon\"></i></span>';\n      let modeHTML;\n      mode === 'true' ? modeHTML = darkHTML : modeHTML = lightHTML;\n      $dm_btn.html(modeHTML);\n    }\n\n    function setMode(mode) {\n      localStorage.setItem('dark_mode',`${mode}`);\n      document.querySelector('#darkmode').disabled=mode;\n\n      if (mode==='true') {\n        document.querySelector('#darkmode').disabled=false;\n        $dm_btn.html('<span class=\"which-mode\"> Dark Mode<i id=\"mode_icon\" class=\"fa fa-moon mode-icon\"></i></span>');\n\n        // document.querySelector('link[href=\"css/theme-dark-mode.css\"]').disabled = false;\n      } else {\n        $dm_btn.html('<span class=\"which-mode\"> Light Mode<i id=\"mode_icon\" class=\"fa fa-sun mode-icon\"></i></span>');\n      }\n      return setModeText(mode);\n    }\n\n    // Theme switcher\n    $dm_btn.on('click', (event) => {\n      event.preventDefault();\n      if (localStorage.getItem('dark_mode')==='true') {\n        setMode('false'), console.log('set to false');\n      } else {\n        setMode('true'), console.log('set to true');\n      }\n      return;\n    });\n    \n  // });\n// eslint-disable-next-line no-undef\n} \nconst _ThemeSwitcher=ThemeSwitcher;\nexport {_ThemeSwitcher as ThemeSwitcher};\nThemeSwitcher();\n"},"lineCount":null}},"error":null,"hash":"9e3c03c4c6f21dce236276fcd8142672","cacheData":{"env":{}}}