  "version": "2.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "start": "npm-run-all --sequential comp:clean comp:build:nowatch eslint js:transpile gulp:ejsit comp:copy scss:compile assets:copy serve:proxy",
    "start:debug": "npm run build && node build-scripts/start-debug.js",
    "comp:build:nowatch": "npm run comp:clean && node_modules/.bin/stencil build --dev --docs-readme --debug",
    "comp:build": "npm run comp:clean && node_modules/.bin/stencil build --docs-readme --watch",
    "comp:serve": "node_modules/.bin/stencil build --dev --watch --serve",
    "comp:test": "node_modules/.bin/stencil test --spec --e2e",
    "comp:test:watch": "node_modules/.bin/stencil test --spec --e2e --watchAll",
    "comp:generate": "node_modules/.bin/stencil generate",
    "comp:copy": "node_modules/.bin/copy www/components/**/* www-app/components",
    "comp:copy:build:nowatch": "npm-run-all comp:build:nowatch comp:copy",
    "build": "npm-run-all --sequential comp:build:nowatch comp:copy html:components:copy js:transpile gulp:ejsit stylelint:go scss:compile autoprefixer:go assets:copy",
    "assets:copy": "node ./build-scripts/build-assets.js && npm run gulp:copy:js",
    "ejs:build": "node build-scripts/build-ejs.js",
    "eslint": "eslint --fix src/js/HeathScript.js src/js/jqBootstrapValidation.js src/js/contact_me.js src/js/modules/show-more-fadebar/show-more.js",
    "js:transpile": "npm-run-all --sequential eslint js",
    "js": "node build-scripts/build-scripts",
    "gulp:babelfry": "gulp babelfry",
    "stylelint": " stylelint --fix src/scss/**/*.scss",
    "scss:compile": "node build-scripts/build-scss.js",
    "autoprefixer": "npx postcss www-app/assets/css/*.css --use autoprefixer -d www-app/assets/css/",
    "html:components:build": "gulp build_components",
    "html:components:copy": "gulp copy_components",
    "gulp:copy:js": "gulp copy_js",
    "comp:clean": "node build-scripts/clean-components.js",
    "serve": "gulp serve",
    "serve:php": "php -S localhost:8000 -t www-app/",
    "serve:proxy": "gulp connect_sync",
    "watch": "node build-scripts/watch.js",
    "gwatch": "gulp watchers",
    "gulp:ejsit": "gulp ejsit",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache",
    "scss:vars:convert": "node src/js/modules/scssVars-2-cssVars/index.js",
    "cssvars": "node_modules/.bin/babel-node src/js/modules/sass-to-css-variables/index.js",
    "debug:scss": "sass src/scss/styles.scss www-app/assets/css/HeathStyle.built.css"
  },
  "description": "Heath Shults - .coder - .thinker - .father",
