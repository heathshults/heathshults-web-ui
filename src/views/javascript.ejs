    <script type = "text/javascript"> 
      var db = openDatabase('heathshults.com', '1.0', 'DEV DB', 2 * 1024 * 1024); 
      var msg; 
 
      db.transaction(function (tx) { 
         tx.executeSql('CREATE TABLE IF NOT EXISTS LOGS (id unique, clog)'); 
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (1, "wood")'); 
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (2, "loger")');
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (3, "oak")'); 
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (4, "cedar")');
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (5, "pig")'); 
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (6, "dirty")');
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (7, "moisture")'); 
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (8, "spooky")');
         
         msg = '<p>Log message created and row inserted.</p>'; 
         document.querySelector('#status').innerHTML =  msg; 
      })
      
      db.transaction(function (tx) { 
         tx.executeSql('CREATE TABLE IF NOT EXISTS CLUBS (id unique, club)'); 
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (1, "Curtain Club")'); 
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (2, "Trees")');
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (3, "Club Indigo")'); 
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (4, "Club Ohs")');
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (5, "Lizard Lounge")'); 
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (9, "On the Rocks")');
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (7, "Revolution Room")'); 
         tx.executeSql('INSERT INTO LOGS (id, log) VALUES (8, "Red Blood Club")');
         
         msg = '<p>Log message created and row inserted.</p>'; 
         document.querySelector('#status').innerHTML =  msg; 
      })

      db.transaction(function (tx) { 
         tx.executeSql('SELECT * FROM LOGS', [], function (tx, results) { 
            var len = results.rows.length, i; 
            msg = "<p>Found rows: " + len + "</p>"; 
            document.querySelector('#status').innerHTML +=  msg; 
   
            for (i = 0; i < len; i++) { 
               msg = "<p><b>" + results.rows.item(i).log + "</b></p>"; 
               document.querySelector('#status').innerHTML +=  msg; 
            } 
         }, null); 
      }); 
      db.transaction(function (tx) { 
         tx.executeSql('SELECT * FROM CLUBS', [], function (tx, results) { 
            var len = results.rows.length, i; 
            msg = "<p>Found rows: " + len + "</p>"; 
            document.querySelector('#status').innerHTML +=  msg; 
   
            for (i = 0; i < len; i++) { 
               msg = "<p><b>" + results.rows.item(i).log + "</b></p>"; 
               document.querySelector('#status').innerHTML +=  msg; 
            } 
         }, null); 
      }); 
   </script> 
